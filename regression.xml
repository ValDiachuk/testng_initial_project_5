<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression">
    <test name="All Functionalities">
        <classes>
            <class name="scripts.TGLoginFormTest">
                <methods>
                    <exclude name="validateLoginForm"/>
                    <exclude name="validateTechGlobalLoginFormCardValidLogin"/>
                    <exclude name="validateTechGlobalLoginFormCardValidLoginAndLogOut"/>
                    <exclude name="validateLoginFormForgotPassword"/>
                    <exclude name="validateLoginFormResetPasswordLink"/>
                    <exclude name="validateLoginFormWithWrongUN"/>
                    <exclude name="validateLoginFormInvalidLoginWrongPassword"/>
                    <exclude name="Validate TechGlobal Login Form card invalid login with both wrong credentials"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>